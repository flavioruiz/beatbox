<h2 class="mb20">Offer <%= h(@offer.name) %></h2>

<% form_for @offer, :url => offer_path(@offer) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  
  <div class="section form_control">
    <%= f.submit "Update", :class => 'rounded_corners mt20' %>
  </div>
<% end %>
